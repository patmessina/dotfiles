# general

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

alias ls="ls --color"
alias ll="ls -l"

alias rm="rm -i"
alias cp="cp -i"
alias mkdir="mkdir -p"

alias c="clear"

alias grep='grep --color=auto'
alias ip='ip -color=auto'

alias df="df -h"
alias du="du -h"

# git
#
alias g="git"
alias gs="git status"
alias ga="git add"
alias gcm="git checkout main"
alias gcd="git checkout develop"
alias gcs="git checkout stage"
alias gc="git checkout"
alias gcmsg="git commit -m"
alias gp="git push"
